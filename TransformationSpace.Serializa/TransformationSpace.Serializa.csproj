<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.1.2</Version>
    <Company>ZoyuJ</Company>
    <Authors>ZoyuJ</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/ZoyuJ/TransformationSpace</RepositoryUrl>
    <PackageIcon>Icon_Serializa-2.png</PackageIcon>
    <Description>Serialize and deserialize implemention of TransformationSpace object.
base on newtonsoft.json</Description>
